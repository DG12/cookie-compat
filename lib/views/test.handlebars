<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title></title>
</head>
<body>

{{{cleanUpIFramesHtml}}}

<script id="cookies" type="application/json">
    {{{cookies}}}
</script>

<script>
    var iframes = document.querySelectorAll('iframe');
    var pendingIFramesCount = iframes.length;

    Array.prototype.slice.call(iframes).forEach(function (iframe) {
        iframe.onload = function () {
            if (--pendingIFramesCount === 0)
                runTest();
        };
    });

    function runTest () {
        // NOTE: assert cookies are clean
        if (document.cookie !== '')
            throw new Error('Cookie are not clean: ' + document.cookie);

        var cookies = JSON.parse(document.querySelector('#cookies').textContent);

        cookies.forEach(function (c) {
            document.cookie = c;
        });

        window.location = '{{{resultUrl}}}';
    }
</script>
</body>
</html>